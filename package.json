{
  "name": "free-desktop-entry",
  "productName": "FreeDesktopEntry",
  "version": "0.0.0",
  "description": "freedesktop.org Desktop Menu Manager",
  "license": "MIT",
  "repository": "IvanGaravito/free-desktop-entry",
  "author": {
    "name": "IvanGaravito",
    "email": "ivangaravito@gmail.com",
    "url": "github.com/IvanGaravito"
  },
  "main": "src/main/index.js",
  "scripts": {
    "test": "xo",
    "start": "electron . -r",
    "debug": "electron . --debug -r",
    "build": "electron-packager . --out=dist --asar --overwrite --all"
  },
  "files": [
    "index.js",
    "index.html",
    "index.css"
  ],
  "keywords": [
    "electron-app",
    "electron"
  ],
  "dependencies": {
    "electron": "^1.4.13",
    "electron-compile": "^4.0.3",
    "electron-debug": "^1.0.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-photonkit": "^0.4.1"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-runtime": "^6.20.0",
    "devtron": "^1.1.0",
    "electron-compilers": "^5.0.0",
    "electron-packager": "^8.0.0",
    "electron-prebuilt": "^1.4.13",
    "xo": "^0.16.0"
  },
  "xo": {
    "esnext": true,
    "space": true,
    "semicolon": false,
    "rules": {
      "space-before-function-paren": "off"
    },
    "envs": [
      "node",
      "browser"
    ]
  }
}
